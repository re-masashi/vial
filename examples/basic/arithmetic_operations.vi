let a = 10;
let b = 5;

let addition = a + b;      // 10 + 5 = 15
let subtraction = a - b;   // 10 - 5 = 5
let multiplication = a * b; // 10 * 5 = 50
let division = a / b;      // 10 / 5 = 2

// More complex expression
let complex = (a + b) * (a - b) / 3;  // (15) * (5) / 3 = 25

// Return result of complex calculation
complex
